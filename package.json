{
  "name": "kakarotextension",
  "description": "Mangafire extension",
  "author": "Karrot",
  "keywords": [
    "Paperback",
    "extensions"
  ],
  "version": "0.9.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karrot0/KakarotExtension.git"
  },
  "bugs": {
    "url": "https://github.com/karrot0/KakarotExtension/issues"
  },
  "homepage": "https://github.com/karrot0/KakarotExtension#readme",
  "repositoryName": "KakarotExtension",
  "repositoryDescription": "Mangafire extension",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "format": "prettier --write '**/*{.md,.yaml,.json,.js,.ts}'",
    "format:check": "prettier --check '**/*{.md,.yaml,.json,.js,.ts}'",
    "tools": "rm -rf tools && tsc src/tools/**.ts --outDir tools --target ESNext --module ESNext --moduleResolution bundler --strict true --forceConsistentCasingInFileNames true --skipLibCheck true && node **/tools.js --write",
    "tools:check": "rm -rf tools && tsc src/tools/**.ts --outDir tools --target ESNext --module ESNext --moduleResolution bundler --strict true --forceConsistentCasingInFileNames true --skipLibCheck true && node **/tools.js",
    "bundle": "paperback-cli bundle",
    "serve": "paperback-cli serve",
    "logcat": "paperback-cli logcat",
    "dev": "nodemon -w \"./src/**/*.ts\" -e \"ts\" -x \"npm run serve\"",
    "test": "tsc --project tsconfig.json --noEmit && npm run format:check && npm run bundle",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@paperback/toolchain": "^1.0.0-alpha.33",
    "@paperback/types": "^1.0.0-alpha.33",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "eslint": "9.19.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "dependencies": {
    "@types/node": "^22.10.10",
    "cheerio": "^1.0.0"
  }
}
